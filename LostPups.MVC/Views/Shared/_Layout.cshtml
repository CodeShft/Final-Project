@{
    Layout = null;
}


<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">

    <!--====== Title ======-->
    <title>@ViewData["Title"] - Kayıp Dostlarım</title>

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--====== Favicon Icon ======-->
    <link rel="shortcut icon" href="~/assets/images/favicon.png" type="image/png">

    <!--====== Slick CSS ======-->
    <link rel="stylesheet" href="~/assets/css/slick.css" asp-append-version="true">

    <!--====== Line Icons CSS ======-->
    <link rel="stylesheet" href="~/assets/css/LineIcons.css" asp-append-version="true">

    <!--====== Material Design Icons CSS ======-->
    <link rel="stylesheet" href="~/assets/css/materialdesignicons.min.css" asp-append-version="true">

    <!--====== Jquery Ui CSS ======-->
    <link rel="stylesheet" href="~/assets/css/jquery-ui.min.css" asp-append-version="true">

    <!--====== nice select CSS ======-->
    <link rel="stylesheet" href="~/assets/css/nice-select.css" asp-append-version="true">

    <!--====== Bootstrap CSS ======-->
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css" asp-append-version="true">

    <!--====== Default CSS ======-->
    <link rel="stylesheet" href="~/assets/css/default.css" asp-append-version="true">

    <!--====== Style CSS ======-->
    <link rel="stylesheet" href="~/assets/css/style.css" asp-append-version="true">

</head>

<body>

    <!--====== Preloader Part Start ======-->
    <div class="preloader">
        <div class="loader">
            <div class="ytp-spinner">
                <div class="ytp-spinner-container">
                    <div class="ytp-spinner-rotator">
                        <div class="ytp-spinner-left">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                        <div class="ytp-spinner-right">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== Preloader Part Ends ======-->
    <!--====== Navbar Style 7 Part Start ======-->
    <div class="navigation">
        <header class="navbar-style-7 position-relative">
            <div class="container">
                <!-- navbar mobile Start -->
                <div class="navbar-mobile d-lg-none">
                    <div class="row align-items-center">
                        <div class="col-3">
                            <!-- navbar cart start -->
                            <div class="navbar-toggle icon-text-btn">
                                <a class="icon-btn primary-icon-text mobile-menu-open-7" href="javascript:void(0)">
                                    <i class="mdi mdi-menu"></i>
                                </a>
                            </div>
                            <!-- navbar cart Ends -->
                        </div>
                        <div class="col-6">
                            <!-- desktop logo Start -->
                            <div class="mobile-logo text-center">
                                <a href="index.html"><img src="~/assets/images/logo.png" alt="Logo"></a>
                            </div>
                            <!-- desktop logo Ends -->
                        </div>
                    </div>
                    <!-- navbar search start -->
                    <div class="navbar-search mt-15 search-style-5">
                        <div class="search-select">
                            <select>
                                <option value="">All</option>
                                <option value="1">option 01</option>
                                <option value="2">option 02</option>
                                <option value="3">option 03</option>
                                <option value="4">option 04</option>
                                <option value="5">option 05</option>
                            </select>
                        </div>
                        <div class="search-input">
                            <input type="text" placeholder="Search">
                        </div>
                        <div class="search-btn">
                            <button><i class="lni lni-search-alt"></i></button>
                        </div>
                    </div>
                    <!-- navbar search Ends -->
                </div>
                <!-- navbar mobile Start -->
            </div>

            <div class="navbar-container navbar-sidebar-7">
                <!-- navbar close  Ends -->
                <div class="navbar-close d-lg-none">
                    <a class="close-mobile-menu-7" href="javascript:void(0)"><i class="mdi mdi-close"></i></a>
                </div>
                <!-- navbar close  Ends -->
                <!-- navbar top Start -->
                <div class="navbar-top-wrapper">
                    <div class="container-lg">
                        <div class="navbar-top d-lg-flex justify-content-between">
                            <!-- navbar top left Start -->
                            <div class="navbar-top-left">
                                <ul class="navbar-top-link">
                                    <li><a asp-controller="Home" asp-action="Hakkimizda">HAKKIMIZDA</a></li>
                                    <li><a href="contact-page.html">İLETİŞİM</a></li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="mdi mdi-phone-in-talk"></i>
                                            0850 287 9066
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <!-- navbar top left Ends -->
                            <!-- navbar top right Start -->
                            <div class="navbar-top-right">
                                <ul class="navbar-top-link">
                                    <li>
                                        <a href="#" class="social-login__icon fab fa-instagram">INSTAGRAM</a>
                                        <a href="#" class="social-login__icon fab fa-facebook">FACEBOOK</a>
                                        <a href="#" class="social-login__icon fab fa-twitter">TWITTER</a>
                                        <a href="#" class="social-login__icon fas fa-user"><i class="mdi mdi-account"></i>GİRİŞ</a>
                                    <li>
                                </ul>
                            </div>
                            <!-- navbar top right Ends -->
                        </div>
                    </div>
                </div>
                <!-- navbar top Ends -->
                <!-- main navbar Start -->
                <div class="navbar-wrapper">
                    <div class="container-lg">
                        <nav class="main-navbar d-lg-flex justify-content-between align-items-center">
                            <!-- desktop logo Start -->
                            <div class="desktop-logo w-25 d-none d-lg-block">
                                <a asp-controller="Home" asp-action="Index"><img src="~/assets/images/logo.png" alt="Logo"></a>
                            </div>
                            <!-- desktop logo Ends -->
                            <!-- navbar menu Start -->
                            <div class="navbar-menu">
                                <ul class="main-menu">
                                    <li class="position-static menu-item-has-children">
                                      
                                        <!-- sub mega dropdown Ends -->
                                    </li>
                                    <li><a asp-controller="Home" asp-action="Formlar">Formlar</a></li>
                                    @*<li><a href="category.html">Kids</a></li>
                                    <li><a href="category.html">Accessories</a></li>*@
                                </ul>
                            </div>
                            <!-- navbar menu Ends -->
                            <div class="navbar-search-cart d-none d-lg-flex">
                                <!-- navbar search start -->
                                <div class="navbar-search search-style-5">
                                    <div class="search-input">
                                        <input type="text" placeholder="-Lütfen Aratınız -">
                                    </div>
                                    <div class="search-btn">
                                        <button><i class="lni lni-search-alt"></i></button>
                                    </div>
                                </div>
                                <!-- navbar search Ends -->
                            </div>
                        </nav>
                    </div>
                </div>
                <!-- main navbar Ends -->
            </div>
            <div class="overlay-7"></div>
        </header>
    </div>
    <!--====== Navbar Style 7 Part Ends ======-->


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!--====== Footer Style 3 Part Start ======-->
    <section class="footer-style-3 pt-50 pb-50">
       @* <div class="container">*@
            <div class="footer-top">
                <div class="d-flex justify-content-center">
                    <div class="col-lg-5 col-md-7 col-sm-10">
                        <div class="footer-logo text-center">
                            <a href="index.html">
                                <img class="w-50" src="~/assets/images/logo.png" alt="">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-copyright text-center">
                <p>
                © 2023 Kayıp Dostlarım
            </p>
            </div>
    </section>
    <!--====== Footer Style 3 Part Ends ======-->
    <!--====== Bootstrap 5 js ======-->
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>


    <!--====== Jquery js ======-->
    <script src="~/assets/js/vendor/jquery-3.5.1.min.js"></script>
    <script src="~/assets/js/vendor/modernizr-3.7.1.min.js"></script>

    <!--====== Slick js ======-->
    <script src="~/assets/js/slick.min.js"></script>

    <!--====== Accordion Steps Form js ======-->
    <script src="~/assets/js/jquery-vj-accordion-steps.js"></script>

    <!--====== Jquery Ui js ======-->
    <script src="~/assets/js/jquery-ui.min.js"></script>

    <!--====== Form validator js ======-->
    <script src="~/assets/js/jquery.form-validator.min.js"></script>

    <!--====== nice select js ======-->
    <script src="~/assets/js/jquery.nice-select.min.js"></script>

    <!--====== formatter js ======-->
    <script src="~/assets/js/jquery.formatter.min.js"></script>

    <!--====== Main js ======-->
    <script src="~/assets/js/count-up.min.js"></script>

    <!--====== Main js ======-->
    <script src="~/assets/js/main.js"></script>

</body>

</html>
